{
	"name": "@ctx-core/window",
	"version": "11.0.5",
	"description": "ctx-core window",
	"keywords": [
		"ctx-core",
		"window"
	],
	"homepage": "https://github.com/ctx-core/window#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/window/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/window/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"require": "./dist/index.cjs",
		"import": "./dist/index.js"
	},
	"scripts": {
		"build": "npm run clean && npm run copy-mjs && npm run generate_ctx_I_file && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-mjs": "cd dist && echo \"require = require('esm')(module/*, options*/)\nmodule.exports = require('./main.js')\nexport * from './index.js'\" > index.cjs",
		"exec": "$@",
		"prepublishOnly": "npm run build",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file"
	},
	"dependencies": {
		"@ctx-core/dom": "workspace:^11.0.7",
		"@ctx-core/function": "workspace:^20.0.5",
		"@ctx-core/object": "workspace:^21.0.5",
		"@ctx-core/store": "workspace:^27.0.4",
		"esm": "^3.2.25",
		"svelte": "3.38.3"
	},
	"devDependencies": {
		"@ctx-core/dev-tools": "workspace:^7.0.7",
		"typescript": "^4.3.5"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
