{
	"name": "@ctx-core/window",
	"version": "10.0.9",
	"description": "ctx-core window",
	"keywords": [
		"ctx-core",
		"window"
	],
	"homepage": "https://github.com/ctx-core/window#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/window/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/window/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./src/index.js",
	"module": "./src/index.js",
	"types": "./src/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run generate_ctx_I_file && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run compile",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file"
	},
	"dependencies": {
		"@ctx-core/dom": "workspace:^10.0.8",
		"@ctx-core/function": "workspace:^19.1.7",
		"@ctx-core/object": "workspace:^20.0.8",
		"@ctx-core/store": "workspace:^26.0.9",
		"svelte": "3.38.2"
	},
	"devDependencies": {
		"@ctx-core/dev-tools": "workspace:^6.0.8",
		"typescript": "^4.3.4"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
